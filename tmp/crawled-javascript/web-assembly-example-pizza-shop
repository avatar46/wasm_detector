    <div class="b-section-header__container">
        <div class="b-section-header__wrapper">
            <div class="h-wysiwyg-html">
                <span id="hs_cos_wrapper_widget_1558076382765_b-section-header-richtext" class="hs_cos_wrapper hs_cos_wrapper_widget hs_cos_wrapper_type_rich_text" style="" data-hs-cos-general-type="widget" data-hs-cos-type="rich_text"><h2>Web Assembly Example - Pizza Shop</h2></span>
            </div>
